<h1>Welcome, <%= current_user.username %></h1>
<%= link_to "Sign Out", logout_path %>


<h2>Private Repos:</h2>
<ol>
  <% @private_repos.each do |repo| %>
  <li><%= link_to "#{repo.name}", "#{repo.url}" %></li>
  <% end %>
</ol>

<h2>Public Repos:</h2>
<ol>
  <% @public_repos.each do |repo| %>
  <li><%= link_to "#{repo.name}", "#{repo.url}"  %></li>
  <% end %>
</ol>
